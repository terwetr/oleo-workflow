name: Another Demo
on: [push]
jobs:
  job1:
    name: First Job
    runs-on: ubuntu-latest
    steps:
      - name: Step 1
        uses: actions/checkout@v3
      - name: Step 2
        run: env | sort
  job2:
    name: Second Job
    runs-on: windows-latest
    steps:
      - name: Step One
        uses: actions/checkout@v3
      - name: Step Two
        run: "Get-ChildItem Env: | Sort-Object Name"
  job3:
    name: Third Job
    runs-on: macos-latest
    needs: [job1, job2]
    steps:
      - name: Step Stop
        run: |
          echo "Finished Job 1 & Job 2! üèÜ"
          date